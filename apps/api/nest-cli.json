{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "builder": {
      "type": "swc",
      "options": {
        "extensions": [".js", ".ts", ".jsx", ".tsx"]
      }
    },
    "typeCheck": true,
    "deleteOutDir": true
  }
}
