{
  "name": "@feedbacky/widget-core",
  "description": "A base template for building a shareable web components library with Vite + Svelte",
  "version": "0.0.0",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.umd.js",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "vite --config vite.demo.config.ts",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build -s"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tsconfig/svelte": "^5.0.2",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "files": [
    "dist"
  ]
}