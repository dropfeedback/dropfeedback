{
	"name": "@feedbacky/widget-core",
	"description": "A base template for building a shareable web components library with Vite + Svelte",
	"version": "0.0.0",
	"type": "module",
	"module": "dist/index.js",
	"main": "dist/index.umd.js",
	"scripts": {
		"start": "npm run dev -s",
		"dev": "vite --config vite.config.ts build --watch & vite --config vite.demo.config.ts --open",
		"build": "vite build",
		"build:demo": "vite build --config vite.demo.config.ts",
		"preview": "vite preview --config vite.demo.config.ts",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"prepublishOnly": "npm run build -s",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^2.4.6",
		"@tsconfig/svelte": "^5.0.2",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-preprocess": "^5.0.4",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier-plugin-svelte": "^2.10.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.11"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@ant-design/colors": "^7.0.0",
		"@ctrl/tinycolor": "^4.0.2",
		"svelte-popperjs": "^1.3.2"
	}
}
