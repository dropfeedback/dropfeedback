{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "docker": "docker compose down && docker compose up -d",
    "build-widget": "turbo run build-widget --filter=@feedbacky/widget",
    "db:generate": "turbo run db:generate --filter=@feedbacky/api",
    "db:push": "turbo run db:push --filter=@feedbacky/api",
    "db:seed": "turbo run db:seed --filter=@feedbacky/api",
    "api:start": "turbo run start:prod --filter=@feedbacky/api"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.12",
    "eslint": "^8.28.0",
    "prettier": "^3.0.0",
    "turbo": "latest"
  },
  "name": "feedbacky",
  "packageManager": "npm@10.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "character-parser": "^4.0.0",
    "is-whitespace": "^0.3.0",
    "preview-email": "^3.0.19"
  }
}
