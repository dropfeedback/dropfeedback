{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "docker": "docker compose down && docker compose up -d",
    "build-widget": "turbo run build-widget --filter=@feedbacky/widget",
    "db:generate": "turbo run db:generate --filter=@feedbacky/api",
    "db:push": "turbo run db:push --filter=@feedbacky/api",
    "db:seed": "turbo run db:seed --filter=@feedbacky/api",
    "api:start": "turbo run start:prod --filter=@feedbacky/api"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@turbo/gen": "^1.10.12",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.28.0",
    "prettier": "^3.0.0",
    "turbo": "latest"
  },
  "name": "feedbacky",
  "packageManager": "npm@8.12.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
